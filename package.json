{
  "name": "univjobsfrontend",
  "version": "1.0.0",
  "description": "React-Redux front end to the Univjobs application.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "proxy": "nodemon proxy.js",
    "production": "webpack -p",
    "test": "cross-env NODE_PATH=app ava",
    "test-watch": "cross-env NODE_PATH=app ava --watch --verbose",
    "test-cov": "cross-env NODE_PATH=app nyc ava",
    "report": "nyc report --reporter=html",
    "lint": "eslint app/.; exit 0",
    "fix": "eslint --fix app/.; exit 0",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnivJobs/univjobs-front.git"
  },
  "keywords": [],
  "author": "Khalil Stemmler, Charles Javelona, Julio Sueiras",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UnivJobs/univjobs-front/issues"
  },
  "ava": {
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/helpers/setup.js"
    ],
    "babel": "inherit"
  },
  "xo": {
    "extends": "xo-react"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "ignore": "tests/**/*.js"
  },
  "homepage": "https://github.com/UnivJobs/",
  "dependencies": {
    "axios": "^0.12.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "expose-loader": "0.7.1",
    "express": "^4.14.0",
    "firebase": "^2.4.2",
    "lodash": "^3.10.1",
    "lowdb": "^0.13.1",
    "moment": "^2.14.1",
    "radium": "^0.18.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-cookie": "^0.4.7",
    "react-dom": "^15.4.1",
    "react-dropzone": "^3.5.1",
    "react-redux": "^4.4.5",
    "react-redux-toastr": "^3.9.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.5",
    "react-skylight": "^0.4.0",
    "react-text-mask": "^0.17.2",
    "react-toastr": "^2.8.0",
    "react-tooltip": "^3.1.6",
    "react-widgets": "^3.4.4",
    "redux": "^3.5.2",
    "redux-form": "^5.3.3",
    "redux-thunk": "^2.1.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "6.7.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.15.0",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nyc": "^10.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "redux-ava": "^2.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-bundle-tracker": "0.0.93",
